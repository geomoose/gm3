    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "fs-extra": "^8.1.0",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "jest": "^28.1.1",
    "json-loader": "^0.5.7",
    "jspdf": "^2.5.1",
    "jsts": "~1.4.0",
    "less": "^3.10.3",
    "mapskin": "^1.0.0",
    "markup-js": "^1.5.21",
    "md5": "^2.2.1",
