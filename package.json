{
  "name": "geomoose",
  "version": "3.2.2-beta",
  "repository": "https://github.com/geomoose/gm3.git",
  "description": "GeoMoose is a Web Client JavaScript Framework for displaying distributed cartographic data.",
  "main": "dist/geomoose.js",
  "homepage": "http://www.geomoose.org",
  "bugs": "https://github.com/geomoose/gm3/issues",
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest",
    "test:only": "jest",
    "lint": "eslint --config ./eslint.config.js src/gm3 tests",
    "pretest": "npm run lint",
    "prepack": "grunt build",
    "start": "webpack-dev-server --config ./webpack.config.js",
    "serve": "grunt serve"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": []
  },
  "jest": {
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "node_modules/(?!ol|mapbox|jsts)"
    ],
    "moduleDirectories": [
      "./src",
      "./node_modules"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "author": "Dan 'Ducky' Little",
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "example": "demo",
    "lib": "src",
    "test": "tests"
  },
  "devDependencies": {
    "@mapbox/mapbox-gl-style-spec": "^9.0.0",
    "@turf/buffer": "^5.1.5",
    "@turf/union": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "grunt": "^1.0.2",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-webpack": "^3.1.1",
    "gruntify-eslint": "^4.0.0",
    "jest": "^22.4.3",
    "json-loader": "^0.5.7",
    "jspdf": "^1.3.5",
    "jsts": "~1.4.0",
    "mapbox-to-ol-style": "^3.4.3",
    "mapskin": "^1.0.0",
    "markup-js": "^1.5.21",
    "md5": "^2.2.1",
    "papaparse": "^4.3.7",
    "proj4": "^2.4.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "reqwest": "^2.0.5",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "usng-tools-js": "git+https://github.com/klassenjs/usng-tools-js.git",
    "uuid": "^3.2.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "xmlshim": "^0.2.5"
  },
  "dependencies": {
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jsdom": "^13.1.0",
    "ol": "^5.3.0",
    "ol-mapbox-style": "^3.5.0"
  }
}
